        android:layout_x="160dp"
        android:layout_y="207dp"
        android:text="Switch" />

    <Button
        android:id="@+id/SLunch"
        android:layout_width="218dp"
        android:layout_height="wrap_content"
        android:layout_x="356dp"
        android:layout_y="200dp"
