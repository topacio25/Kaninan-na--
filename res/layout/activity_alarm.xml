    <Button
        android:id="@+id/LunchTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="13dp"
        android:layout_y="200dp"
        android:text="Set Time" />

    <TextView
        android:id="@+id/LunchText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
