    <Button
        android:id="@+id/SnackTime"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_x="20dp"
        android:layout_y="374dp"
        android:text="Set Time" />

    <TextView
        android:id="@+id/SnackText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
