    <Button
        android:id="@+id/Bfast"
        android:layout_width="165dp"
        android:layout_height="wrap_content"
        android:layout_x="10dp"
        android:layout_y="730dp"
        android:onClick="nextFrame"
        android:text="Remove Baby" />

</AbsoluteLayout>